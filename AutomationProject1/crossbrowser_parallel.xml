<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Crossbrowser Tests" parallel = "tests" thread-count="4">
  
    
  <test thread-count="6" name="Chrome Test">
    <parameter name="browser" value="chrome"/>
      <classes>
        <class name="tests.KaterinaSprint2"/>
     </classes>
  </test> <!-- Test -->
  
  <test thread-count="6" name="Firefox Test">
    <parameter name="browser" value="firefox"/>
      <classes>
        <class name="tests.KaterinaSprint2"/>
     </classes>
  </test> <!-- Test -->
  
  <test thread-count="6" name="Edge Test">
    <parameter name="browser" value="edge"/>
      <classes>
        <class name="tests.KaterinaSprint2"/>
     </classes>
  </test> <!-- Test -->
  
  <test thread-count="6" name="Headless Chrome Test">
    <parameter name="browser" value="headlesschrome"/>
      <classes>
        <class name="tests.KaterinaSprint2"/>
     </classes>
  </test> <!-- Test -->
  

  
</suite> <!-- Suite -->